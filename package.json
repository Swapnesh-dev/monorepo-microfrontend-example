{
  "name": "monorepo",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bootstrap": "pnpm install",
    "dev:remote": "pnpm --filter=apps/remote dev",
    "preview:remote": "pnpm --filter=apps/remote preview",
    "dev:container": "pnpm --filter=apps/container dev",
    "preview:container": "pnpm --filter=apps/container preview",
    "build:remote": "pnpm --filter=apps/remote build",
    "build:container": "pnpm --filter=apps/container build",
    "deploy:remote": "pnpm install && pnpm run build:remote",
    "deploy:container": "pnpm install && pnpm run build:container",
    "mf:remote": "pnpm run build:remote && pnpm run preview:remote",
    "mf:container": "pnpm run build:container && pnpm run preview:container"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": ""
}
