{
  "name": "monorepo",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:remote": " pnpm run dev --workspace=apps/remote",
    "dev:container": " pnpm run dev --workspace=apps/container",
    "preview:remote": " pnpm run preview --workspace=apps/remote",
    "install:container": " pnpm run install --workspace=apps/container",
    "install:remote": " pnpm run install --workspace=apps/remote",
    "preview:container": " pnpm run preview --workspace=apps/container",
    "build:remote": " pnpm run build --workspace=apps/remote",
    "build:container": " pnpm run build --workspace=apps/container",
    "mf:remote": "pnpm run build:remote && pnpm run preview:remote",
    "mf:container": "pnpm run build:container && pnpm run preview:container"
  },
